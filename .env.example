# Prestige Flowers Scraper Configuration
# Copy this file to .env and modify as needed

# Database configuration
SCRAPER_DB_PATH=prestige_flowers.db

# Scraping targets
SCRAPER_CATEGORY_URL=https://www.prestigeflowers.co.uk/christmas-plants

# Processing limits
SCRAPER_DETAILS_LIMIT=None
SCRAPER_CHECK_RECENT=True

# Output configuration
SCRAPER_CSV_FILENAME=prestige_flowers_combined.csv